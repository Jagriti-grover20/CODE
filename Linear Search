//https://hack.codingblocks.com/app/contests/2707/398/problem
Loop will repeatedly iterate for all the number present or not in the array so in order to print index of only particular element 
Use flag and define outside the loop
#include<iostream>
using namespace std;
int main() {
	int n,flag,pos;
	cin>>n;
	int arr[100000];
	for(int i=0;i<n;i++)
	{
		cin>>arr[i];
	}
	int search;
	cin>>search;
	for(int j=0;j<n;j++)
	{
		if(arr[j]==search)
		{
         flag=1;
         pos=j;
         break;
		}
	
	}
	if(flag==1)
	{.
		cout<<pos;
	}
		else
		{
			cout<<"-1"<<endl;
		   
		}
	return 0;
}
